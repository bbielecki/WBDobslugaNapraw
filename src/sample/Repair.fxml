<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane minHeight="500.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RepairController">
   <bottom>
      <AnchorPane minHeight="180.0" minWidth="600.0" prefHeight="229.0" prefWidth="638.0" BorderPane.alignment="CENTER">
         <children>
            <ChoiceBox fx:id="VehicleModelChoice" layoutX="150.0" layoutY="49.0" prefWidth="150.0" AnchorPane.leftAnchor="150.0" AnchorPane.topAnchor="49.0" />
            <ChoiceBox fx:id="statusChoiceBox" layoutX="150.0" layoutY="81.0" prefWidth="150.0" AnchorPane.leftAnchor="150.0" AnchorPane.topAnchor="81.0" />
            <Label layoutX="14.0" layoutY="47.0" prefHeight="25.0" prefWidth="136.0" text="Vehicle Model" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="47.0" />
            <Label layoutX="14.0" layoutY="79.0" prefHeight="25.0" prefWidth="136.0" text="Vehicle status" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="79.0" />
         </children>
      </AnchorPane>
   </bottom>
   <center>
      <AnchorPane minHeight="300.0" minWidth="600.0" prefHeight="300.0" prefWidth="637.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="checkRepairHistoryButton" layoutX="448.0" layoutY="225.0" mnemonicParsing="false" onAction="#checkHistoryAction" prefHeight="39.0" prefWidth="136.0" text="Check Repair History" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0" />
            <TableView fx:id="RepairTable" layoutX="11.0" layoutY="34.0" prefHeight="182.0" prefWidth="576.0" AnchorPane.bottomAnchor="72.0" AnchorPane.leftAnchor="11.0" AnchorPane.rightAnchor="11.0" AnchorPane.topAnchor="34.0" />
            <Button fx:id="ShowButton" layoutX="20.0" layoutY="247.0" mnemonicParsing="false" onAction="#showVehicles" prefHeight="39.0" prefWidth="115.0" text="Show Vehicles" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="20.0" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
